Algoritmo "area"

Var

   //Exemplo 1

   h, b1, b2, area : real
   
   //Exemplo 2
   
   a: real
   b: inteiro

Inicio

   //Exemplo 1

   h <- 5.0
   b1 <- 6.0
   b2 <- 8.0
   
   area <- (b1+b2) /2 *h;
   
   escreval(area)
   
   //Exemplo 2
   
   a <- 5.3
   b <- Int(a)
   
   escreval(b)


Fimalgoritmo
